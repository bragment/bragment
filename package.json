    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-intl": "^6.0.4",
    "react-markdown": "^8.0.0",
    "react-progressive-image": "^0.6.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "react-spring": "^9.4.1",
    "rehype-highlight": "^5.0.2",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "source-map-explorer": "^2.5.2",
    "standard-version": "^9.3.2",
    "typescript": "4.5.4",
