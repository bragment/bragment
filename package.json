{
  "name": "bragment",
  "version": "0.2.0-0",
  "description": "bragment web app",
  "repository": "https://github.com/bragment/bragment",
  "author": "YickFung <lamyickfung@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer build/**/*.js",
    "postinstall": "husky install",
    "commit": "cz",
    "commit:amend": "git commit --amend --no-edit",
    "commit:first": "yarn version --prerelease --no-git-tag-version",
    "lint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "release": "standard-version",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage",
    "echo:version": "node -p \"require('./package.json').version\"",
    "generate:schema:types": "node -r dotenv/config node_modules/.bin/graphql-codegen"
  },
  "dependencies": {},
  "devDependencies": {
    "@apollo/client": "^3.5.6",
    "@breeze2/react-beautiful-dnd": "13.1.0-3",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@craco/craco": "^6.4.3",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.3",
    "@sentry/cli": "^2.2.0",
    "@sentry/react": "^7.2.0",
    "@sentry/tracing": "^7.2.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^18.0.0",
    "@types/parse": "^2.18.13",
    "@types/react": "^18.0.0",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.2",
    "antd": "^4.21.3",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.4",
    "craco-antd": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^16.2.0",
    "husky": "^8.0.1",
    "mobx": "^6.3.12",
    "mobx-react": "^7.2.1",
    "parse": "^3.4.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
