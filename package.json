  "author": "YickFung <lamyickfung@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "commit": "cz",
    "commit:amend": "git commit --amend --no-edit",
    "commit:start": "yarn version --no-git-tag-version --preminor",
    "commit:end": "yarn version --no-git-tag-version --minor",
    "postcommit:start": "git add package.json && git commit -m \"chore(version): start $(yarn run --silent echo:version)\"",
    "postcommit:end": "git add package.json && git commit -m \"chore(version): end $(yarn run --silent echo:version)\"",
    "release": "standard-version",
    "lint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage",
    "echo:version": "node -p \"require('./package.json').version\""
  },
  "proxy": "http://localhost:3001",
  "dependencies": {},
